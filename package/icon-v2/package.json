{
  "name": "oh-vue-icon-v2",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:lib-css && npm run copy",
    "build:lib": "vue-cli-service build --target lib --name icon ./src/index.js",
    "build:lib-css": "EXTRACT_CSS=true npm run build:lib",
    "build:demo": "vue-cli-service build",
    "copy": "cp dist/icon.umd.min.js ../dist/v2/icon.umd.min.js && cp dist-css/icon.umd.min.js ../dist-css/v2/icon.umd.min.js && cp dist-css/icon.css ../dist-css/v2/icon.css",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  }
}
