{
  "name": "oh-vue-icons",
  "version": "0.1.8",
  "description": "Include inline SVG icons from different icon packs in Vue easily.",
  "keywords": [
    "font awesome",
    "remix icon",
    "academicons",
    "game icons",
    "vue",
    "icons"
  ],
  "homepage": "https://oh-vue-icons.vercel.app",
  "bugs": {
    "url": "https://github.com/Renovamen/oh-vue-icons/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Renovamen/oh-vue-icons"
  },
  "license": "MIT",
  "author": "Renovamen <renovamenzxh@gmail.com>",
  "scripts": {
    "build": "node scripts/build.js",
    "build:docs": "nuxt generate",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "nuxt",
    "download": "sh ./scripts/download.sh",
    "lint": "eslint --ext .js,.vue ./",
    "lint-fix": "eslint --fix --ext .js,.vue ./",
    "preview:docs": "cd docs/dist && http-server",
    "pub": "sh ./scripts/publish.sh",
    "start": "nuxt start",
    "submodule": "git submodule update --init"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix --ext .js,.vue ./"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "core-js": "^3.4.4"
  },
  "devDependencies": {
    "@nuxt/content": "^1.11.1",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.1",
    "eslint": "^7.19.0",
    "eslint-plugin-vue": "^7.5.0",
    "glob-promise": "^3.4.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nuxt": "^2.14.6",
    "rimraf": "^2.7.1",
    "sort-package-json": "^1.48.1",
    "svgo": "^1.3.2",
    "vue-slider-component": "^3.2.11",
    "vue-swatches": "^2.1.0"
  },
  "peerDependencies": {
    "vue": "^2.4.0"
  }
}
