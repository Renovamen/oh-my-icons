{
  "name": "oh-vue-icons",
  "version": "0.1.2",
  "description": "Include inline SVG icons from different icon packs in Vue easily, based on vue-awesome.",
  "author": "Renovamen <renovamenzxh@gmail.com>",
  "license": "MIT",
  "homepage": "https://oh-vue-icons.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Renovamen/oh-vue-icons"
  },
  "bugs": {
    "url": "https://github.com/Renovamen/oh-vue-icons/issues"
  },
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:docs": "nuxt build",
    "generate:docs": "nuxt generate",
    "gen-svgs": "node -r esm scripts/gen-svgs.js",
    "icons": "yarn gen-svgs && node -r esm scripts/gen-js.js",
    "pub": "sh ./scripts/publish.sh"
  },
  "main": "dist/oh-vue-icons.js",
  "module": "index.js",
  "dependencies": {
    "core-js": "^3.4.4"
  },
  "devDependencies": {
    "academicons": "git+https://github.com/jpswalsh/academicons.git",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "cross-env": "^5.2.1",
    "esm": "^3.2.25",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.1",
    "nuxt": "^2.14.6",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "remixicon": "^2.5.0",
    "rimraf": "^2.7.1",
    "rollup": "^2.22.1",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "semver": "^5.7.1",
    "stringify-object": "^3.3.0",
    "svgpath": "^2.3.0",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "xml2js": "^0.4.23"
  },
  "peerDependencies": {
    "vue": "^2.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "font awesome",
    "remix icon",
    "academicons",
    "vue",
    "icons"
  ]
}