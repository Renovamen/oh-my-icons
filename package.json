{
  "name": "oh-vue-icons",
  "version": "0.1.2",
  "description": "Include inline SVG icons from different icon packs in Vue easily, based on vue-awesome.",
  "author": "Renovamen <renovamenzxh@gmail.com>",
  "license": "MIT",
  "homepage": "https://oh-vue-icons.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Renovamen/oh-vue-icons"
  },
  "bugs": {
    "url": "https://github.com/Renovamen/oh-vue-icons/issues"
  },
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "build": "node -r esm scripts/build.js",
    "build:docs": "nuxt generate",
    "preview:docs": "cd docs/dist && http-server",
    "download": "sh ./scripts/download.sh",
    "pub": "sh ./scripts/publish.sh",
    "submodule": "git submodule update --init"
  },
  "main": "dist/oh-vue-icons.js",
  "module": "index.js",
  "dependencies": {
    "core-js": "^3.4.4"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^3.1.0",
    "cross-env": "^5.2.1",
    "esm": "^3.2.25",
    "glob-promise": "^3.4.0",
    "nuxt": "^2.14.6",
    "prismjs": "^1.22.0",
    "rimraf": "^2.7.1",
    "svgpath": "^2.3.0",
    "svgo": "^1.3.2",
    "vue-lazyload": "1.2.6",
    "xml2js": "^0.4.23"
  },
  "peerDependencies": {
    "vue": "^2.4.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "font awesome",
    "remix icon",
    "academicons",
    "game icons",
    "vue",
    "icons"
  ]
}